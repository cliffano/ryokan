{
  "commands": {
    "delicious-screenshot": {
      "desc": "Take screenshots of Delicious.com bookmarks",
      "options": [
        { "arg": "-u, --username <username>", "desc": "Delicious.com account username" },
        { "arg": "-p, --password <password>", "desc": "Delicious.com account password" },
        { "arg": "-t, --tags <tags>", "desc": "Bookmark tags (comma-separated)" }
      ],
      "examples": [
        "ryokan delicious-screenshot -u someuser -p s0m3p455 -t tag1,tag2,tag3"
      ]
    },
    "delicious-delete": {
      "desc": "Delete Delicious.com bookmarks",
      "options": [
        { "arg": "-u, --username <username>", "desc": "Delicious.com account username" },
        { "arg": "-p, --password <password>", "desc": "Delicious.com account password" },
        { "arg": "-t, --tags <tags>", "desc": "Bookmark tags (comma-separated)" }
      ],
      "examples": [
        "ryokan delicious-delete -u someuser -p s0m3p455 -t tag1,tag2,tag3"
      ]
    }
  }
}